<?xml version="1.0" encoding="UTF-8"?>
<persistence>
     
        
    <persistence-unit name="jpa-db">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
                
        <class>test.model.User</class>
                
        <properties>
                        
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
                        
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5434/springbootdb"/>
                        
            <property name="javax.persistence.jdbc.user" value="sb_user"/>
                        
            <property name="javax.persistence.jdbc.password" value="sb_pass"/>
                        
            <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQL10Dialect"/>
                        
            <property name="hibernate.show_sql" value="true"/>
                    
        </properties>
    </persistence-unit>

        
     
</persistence>